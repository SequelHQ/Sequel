{
  "name": "sequel",
  "version": "0.0.2",
  "productName": "Sequel",
  "private": true,
  "author": "Sequel",
  "description": "Sequel desktop app",
  "homepage": "./",
  "main": "./public/electron.js",
  "dependencies": {
    "@electron-forge/publisher-s3": "^7.2.0",
    "@getstation/electron-google-oauth2": "^14.0.0",
    "@headlessui/react": "^1.7.18",
    "@headlessui/tailwindcss": "^0.2.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tailwindcss/forms": "^0.5.7",
    "@tremor/react": "^3.14.1",
    "@uiw/react-markdown-preview": "^5.0.3",
    "appdmg": "^0.6.6",
    "aws-sdk": "^2.1555.0",
    "axios": "^1.6.0",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.3.2",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.5",
    "electron-squirrel-startup": "^1.0.0",
    "groq-sdk": "^0.3.2",
    "katex": "^0.16.9",
    "lodash": "^4.17.21",
    "lucide-react": "^0.356.0",
    "mic-recorder-to-mp3": "^2.2.2",
    "openai": "^4.29.0",
    "pdfjs-dist": "^4.0.379",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-latex-next": "^2.2.0",
    "react-markdown": "^9.0.1",
    "react-modal": "^3.16.1",
    "react-router": "^6.18.0",
    "react-router-dom": "^6.18.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^10.0.4",
    "stripe": "^14.18.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "update-electron-app": "^3.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "devstart": "react-scripts start",
    "publish": "electron-forge publish",
    "publish-full": "node publish/full-publish.js",
    "publish-no-release": "node publish/pre-build.js && react-scripts build && electron-forge publish && node publish/update-releases.js",
    "publish-existing-release": "node publish/release.js",
    "start": "electron-forge start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "electron": "electron .",
    "electron:start": "electronmon .",
    "electron:package:mac": "yarn build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "yarn build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "yarn build && electron-builder -l -c.extraMetadata.main=build/electron.js",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "format": "eslint --fix ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-dmg": "^6.4.2",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@electron-forge/plugin-auto-unpack-natives": "^6.4.2",
    "@types/lodash": "^4.14.202",
    "@types/pdfjs-dist": "^2.10.378",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "@types/react-modal": "^3.16.3",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "27.1.2",
    "electron-builder": "^24.6.4",
    "electron-installer-dmg": "^4.0.0",
    "electron-is-dev": "^2.0.0",
    "electronmon": "^2.0.2",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "postcss-loader": "^7.3.3",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.3.2",
    "wait-on": "^7.0.1"
  },
  "build": {
    "appId": "com.sequel.app",
    "productName": "Sequel",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "./public/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "target": "dmg",
      "icon": "public/images/MyApp.icns"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb"
    }
  }
}
